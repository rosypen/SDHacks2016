{
  "manifest_version": 2,

  "name": "Hack Hate Speech",
  "description" : "This extension finds hate speech text and collapses it",
  "version": "1.0",

  "browser_action": {
    "default_icon": "unnamed.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"
  ]
}
